{
  "name": "rtx-test",
  "version": "1.0.0",
  "description": "rtx-test usage only",
  "main": "index.js",
  "scripts": {
    "coverage": "nyc npm run test",
    "create-db": "sequelize db:create",
    "db:create:test": "NODE_ENV=test npx sequelize-cli db:create",
    "db:reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate",
    "dev": "nodemon ./Server/index.js",
    "migrate-db": "sequelize db:migrate",
    "pretest": "NODE_ENV=test npm run db:reset",
    "test": "NODE_ENV=test nyc mocha --timeout 10000 ./tests/*"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.1",
    "dotenv": "^16.0.1",
    "express": "^4.17.2",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.7",
    "passport": "^0.5.2",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.21.3",
    "sequelize-auto-migrations-v2": "^1.2.1"
  },
  "devDependencies": {
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.15",
    "nyc": "^15.1.0",
    "sequelize-cli": "^6.4.1"
  }
}
